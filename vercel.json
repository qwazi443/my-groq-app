{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/samples/(.*)",
      "headers": [
        { "key": "Content-Type", "value": "audio/wav" },
        { "key": "Content-Disposition", "value": "inline" }
      ]
    }
  ]
}